project(*Publisher): dcpsexe, dcps_test, dcps_transports_for_test {
  exename = FooTest3_publisher
  after    += DcpsFooType3Unbounded
  libs     +=  DcpsFooType3Unbounded
  libpaths += ../FooType3Unbounded

  Source_Files {
    pub_main.cpp
    PubDriver.cpp
    InstanceDataMap.cpp
    Writer.cpp
  }
}

project(*Subscriber): dcpsexe, dcps_test, dcps_transports_for_test {
  exename = FooTest3_subscriber
  after    += DcpsFooType3Unbounded *Publisher
  libs     += DcpsFooType3Unbounded
  libpaths += ../FooType3Unbounded

  Source_Files {
    sub_main.cpp
    SubDriver.cpp
    DataReaderListener.cpp
  }
}
